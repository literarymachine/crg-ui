@import './Colors.pcss';

.Header {
  & .subHeader {
    box-shadow: 0 3px 5px rgba(0, 0, 0, 0.16), 0 3px 5px rgba(0, 0, 0, 0.23);
    background-color: color(color(var(--blue) l(+8%)) s(60%));
    padding: 10px;
    text-align: right;
  }

  & a {
    color: var(--white);
    text-decoration: none;

    &:hover {
      color: var(--orange);
    }
  }
}
